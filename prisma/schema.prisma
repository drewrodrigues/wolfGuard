datasource db {
  provider = "postgresql"
  url      = "postgresql://drew:password@localhost:5432/smarttraderdev?schema=public"
}

generator client {
  provider = "prisma-client-js"
}

model Bar {
  id     Int      @id @default(autoincrement())
  symbol String
  type   String
  time   DateTime
  open   Float
  high   Float
  low    Float
  close  Float
  volume Float

  @@unique([symbol, time, type])
}
